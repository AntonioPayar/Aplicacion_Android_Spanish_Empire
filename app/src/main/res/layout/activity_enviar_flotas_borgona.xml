<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Flotas.EnviarFlotasAragon"
    android:background="@drawable/tablas">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="433dp"
        android:layout_height="33dp"
        android:layout_marginTop="16dp"
        android:fontFamily="monospace"
        android:gravity="center"
        android:text="Mercancía de Borgoña"
        android:textColor="@color/black"
        android:textSize="23sp"
        app:layout_constraintBottom_toTopOf="@+id/seekBar_oroNuevaEspana"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <RadioGroup
        android:id="@+id/radioGroupBorgona"
        android:layout_width="200dp"
        android:layout_height="261dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.666"></RadioGroup>

    <Button
        android:id="@+id/embarcarBotonBorgona"
        android:layout_width="161dp"
        android:layout_height="64dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/boton_mercancias"
        android:text="Embarcar"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/radioGroupBorgona"
        app:layout_constraintHorizontal_bias="0.787"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/contenedorBorgona"
        android:layout_width="184dp"
        android:layout_height="235dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.682">
    </LinearLayout>

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="68dp"
        android:layout_height="59dp"
        android:layout_marginTop="4dp"
        android:background="?selectableItemBackground"
        android:onClick="volverAtras"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/radioGroupBorgona"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.802"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.352"
        app:srcCompat="@drawable/atras" />

    <ImageView
        android:id="@+id/imageViewBorgona"
        android:layout_width="316dp"
        android:layout_height="158dp"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/embarcarBotonBorgona"
        app:layout_constraintEnd_toStartOf="@+id/radioGroupBorgona"
        app:layout_constraintHorizontal_bias="0.527"
        app:layout_constraintStart_toEndOf="@+id/contenedorBorgona"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.755" />

</androidx.constraintlayout.widget.ConstraintLayout>